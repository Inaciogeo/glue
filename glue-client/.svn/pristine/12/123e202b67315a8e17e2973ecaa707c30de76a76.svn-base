package br.org.funcate.glue.event;

import java.util.EventObject;

import br.org.funcate.glue.view.AbstractCanvas;

@SuppressWarnings("serial")
public class ThemeChangedEvent extends EventObject {
	
	private AbstractCanvas canvas;

	public ThemeChangedEvent(Object arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	/**
	 * @param canvas
	 *            the canvas to set
	 */
	public void setCanvas(AbstractCanvas canvas) {
		this.canvas = canvas;
	}

	/**
	 * @return the canvas
	 */
	public AbstractCanvas getCanvas() {
		return canvas;
	}
}
