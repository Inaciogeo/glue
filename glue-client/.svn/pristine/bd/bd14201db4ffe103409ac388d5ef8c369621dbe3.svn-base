package br.org.funcate.glue.model.exception;

public class GlueServerException extends Exception {

	private static final long serialVersionUID = -5876985080816294289L;

	public GlueServerException(String msg) {
		super(msg);
	}

	public GlueServerException(String msg, Throwable exc) {
		super(msg, exc);
	}
}
