package br.org.funcate.glue.model.canvas;

public class UndoRedoItem {

	private double boxX1;
	private double boxY1;
	private double resolution;

	public UndoRedoItem(double boxX1, double boxY1, double resolution) {
		setBoxX1(boxX1);
		setBoxY1(boxY1);
		setResolution(resolution);
	}

	public double getBoxX1() {
		return boxX1;
	}

	public void setBoxX1(double boxX1) {
		this.boxX1 = boxX1;
	}

	public double getBoxY1() {
		return boxY1;
	}

	public void setBoxY1(double boxY1) {
		this.boxY1 = boxY1;
	}

	public double getResolution() {
		return resolution;
	}

	public void setResolution(double resolution) {
		this.resolution = resolution;
	}
}
