	/** < Attribute type integer > */

	/**
	 * @name Access
	 * 
	 *       Functions that return the state of the attributes of the ThemeType
	 */

	// @ {

	/**
	 * \brief Function that returns the number of the themeType
	 * 
	 * @return The number of the ThemeType
	 */

	public int getNumber() {
		return _number;
	}

	// @ }

	/**
	 * @name Set
	 * 
	 *       Functions that modify the state of the attributes
	 */

	// @{

	/**
	 * \brief Function that modifies the number of the themeType
	 * 
	 * @param id
	 *            An integer containing the new number of the themeType
	 */

	private ThemeType(int pNum) {
		this._number = pNum;
	}

	// @ }

}
