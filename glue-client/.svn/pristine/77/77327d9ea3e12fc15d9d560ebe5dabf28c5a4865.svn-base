	DEFAULT(0),

	/** < enum value TREE_THEME. > */

	TREE_THEME(1),

	/** < enum value EXTERN_THEME. > */

	EXTERN_THEME(2),

	/** < enum value FILE_THEME. > */

	FILE_THEME(3),

	/** < enum value WMS_THEME. > */

	WMS_THEME(4),

	/** < enum value WFS_THEME. > */

	WFS_THEME(5),
	
	/** < enum value NETWORK_THEME. > */
	
	NETWORK_THEME(6);

